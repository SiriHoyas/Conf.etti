  "bugs": {
    "url": "https://github.com/SiriHoyas/projectSetup/issues"
  },
  "homepage": "https://github.com/SiriHoyas/projectSetup#readme",
  "devDependencies": {
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0"
  },
  "lint-staged": {
    "*.js": [
